{
  "name": "Chrome插件V3",
  "version": "1.0",
  "description": "基于Vite的chrome插件V3 Template",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "css/content_main_script.css"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "options_page": "/template/option.html",
  "permissions": [
    "storage",
    "declarativeContent",
    "bookmarks",
    "tabs",
    "notifications"
  ],
  "host_permissions": [],
  "web_accessible_resources": [
    {
      "resources": [
        "/images/reimu.png"
      ],
      "matches": [
        "<all_urls>"
      ]
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "content_main_script.js"
      ]
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "assets/*"
      ]
    }
  ],
  "action": {
    "default_icon": {
      "16": "/images/reimu.png",
      "32": "/images/reimu.png",
      "48": "/images/reimu.png",
      "128": "/images/reimu.png"
    },
    "default_title": "Enigima-Talk"
  },
  "icons": {
    "16": "/images/reimu.png",
    "32": "/images/reimu.png",
    "48": "/images/reimu.png",
    "128": "/images/reimu.png"
  }
}